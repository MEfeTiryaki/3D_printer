<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
   <mime-type type="text/gcode">
     <comment>GCode files </comment>
     <glob pattern="*.gcode"/>
   </mime-type>
   <mime-type type="application/wavefront-obj">
     <comment>Wavefront obj files</comment>
     <glob pattern="*.obj"/>
   </mime-type>
   <mime-type type="application/x-amf">
     <comment>Additive Manufacturing Format</comment>
     <glob pattern="*.amf"/>
   </mime-type>
</mime-info>